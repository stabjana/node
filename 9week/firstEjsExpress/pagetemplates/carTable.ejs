<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Cars</title>
</head>

<body>
    <h1>All cars</h1>
    <table>
        <thead>
            <tr>
                <th>Model</th>
                <th>License</th>
            </tr>
        </thead>

        <tbody>
            <%cars.forEach(car=> { %>
                <tr>
                    <td>
                        <%= car.model %>
                    </td>
                    <td>
                        <%= car.license %>
                    </td>
                </tr>

                <%})%>

        </tbody>
    </table>
    <a href="/">Home</a>
</body>

</html>